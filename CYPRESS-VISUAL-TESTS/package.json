{
  "name": "cypress-visual-tests-percy",
  "version": "1.0.0",
  "description": "Cypress visual testing",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run-tests": "cypress run",
    "cy:update-base-snapshots": "cypress run --env updateSnapshots=true",
    "cy:run-tests-with-reporter": "cypress run --reporter cypress-image-snapshot/reporter",
    "cy:run-tests-with-no-failure": "cypress run --env failureOnSnapshotDiff=false",
    "cy:percy": "percy exec -- cypress run"
  },
  "keywords": [
    "cypress",
    "visual",
    "testing",
    "snapshots"
  ],
  "author": "Rajiv Singh",
  "license": "ISC",
  "dependencies": {
    "@percy/cypress": "^2.3.1",
    "cypress": "^4.11.0",
    "cypress-image-snapshot": "^3.1.1",
    "prettier": "^2.0.5"
  }
}
