{
  "name": "cypress-ci-cd-dashboard",
  "version": "1.0.0",
  "description": "Cypress with CI/CD and Dashboard service",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox",
    "cy:run:electron": "cypress run --browser electron",
    "cy:run-dashboard": "cypress run --record --key abef0487-3c31-4ba0-974c-3ea62578ff02",
    "start-jenkins-server": "java -jar jenkins.war -httpPort=8080 --enable-future-java"
  },
  "keywords": [
    "cypress",
    "ci",
    "cd",
    "dashboard"
  ],
  "author": "Rajiv Singh",
  "license": "ISC",
  "dependencies": {
    "cypress": "^4.11.0",
    "prettier": "^2.0.5"
  }
}
