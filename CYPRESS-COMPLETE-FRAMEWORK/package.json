{
  "name": "cypress-complete-framework",
  "version": "1.0.0",
  "description": "Complete automation framework with Cypress",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox",
    "cy:run:electron": "cypress run --browser electron",
    "cy:run-dashboard": "cypress run --record --key c3583a5c-39fa-42d3-9590-edd2374ab01d",
    "cy:update-snapshots": "cypress run --env updateSnapshots=true"
  },
  "keywords": [
    "Cypress",
    "Automation",
    "Framework",
    "Dashboard"
  ],
  "author": "Rajiv Singh",
  "license": "ISC",
  "dependencies": {
    "cypress": "^4.11.0",
    "cypress-image-snapshot": "^3.1.1",
    "prettier": "^2.0.5"
  },
  "devDependencies": {
    "cypress-failed-log": "^2.7.0"
  }
}
